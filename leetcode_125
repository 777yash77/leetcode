class Solution {
    public boolean isPalindrome(String s) {
        String s1=s.toLowerCase().replaceAll("[^a-z0-9]","");
     StringBuffer s2=new StringBuffer("");
        for(int i=s1.length()-1;i>=0;i--){
          s2.append(s1.charAt(i));        
        }return s2.toString().equals(s1);
    }
}
